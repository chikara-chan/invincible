{
  "name": "invincible",
  "version": "1.0.0",
  "description": "A series of miscellaneous functional JavaScript utility library.",
  "scripts": {
    "build": "npm run build:es && npm run build:umd",
    "build:es": "babel ./src --out-dir ./lib",
    "build:umd": "webpack --config ./build/webpack.config.js --progress",
    "test": "mocha --compilers js:babel-register",
    "test:watch": "npm run test -- --watch",
    "clean": "rimraf ./dist ./lib",
    "lint": "eslint . --ext .js",
    "prepublish": "npm run clean && npm run build && npm run lint && npm run test"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.16",
    "eslint": "^3.15.0",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4",
    "webpack": "^1.13.1"
  },
  "main": "lib/index.js",
  "file": [
    "lib"
  ],
  "author": "Chikara Chan",
  "homepage": "https://github.com/chikara-chan/invincible",
  "repository": {
    "type": "git",
    "url": "https://github.com/chikara-chan/invincible.git"
  },
  "license": "MIT"
}
